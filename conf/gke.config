/*
Profile to run the workflow using google kubernetes engine.
*/


process {
  executor='k8s'
  cpus = 12
  memory = 16.GB
  queueSize = 2000
  withName: 'mapSplit' {
    cpus = 1
    memory = 1.GB
  }
}

cloud {
  driver = 'google'
  imageId = 'nf-k8s-vapormap-cluster'
  instanceType = 'n1-standard-4'
}

google {
  project = 'your-project-id'
  zone = 'eu-west1-d'
}
